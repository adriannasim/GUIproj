<?xml version="1.0" encoding="UTF-8"?>
<Context>
    <!-- Define the company name -->
    <context-param>
        <param-name>companyName</param-name>
        <param-value>Bong Bong Art Gallery</param-value>
    </context-param>

    <!-- Define the company email -->
    <context-param>
        <param-name>companyEmail</param-name>
        <param-value>enquiries@bongbong.com</param-value>
    </context-param>

    <!-- Define the company copyrights -->
    <context-param>
        <param-name>companyCopyrights</param-name>
        <param-value>Copyright© 2024, Bong Bong Art Gallery</param-value>
    </context-param>
</Context>